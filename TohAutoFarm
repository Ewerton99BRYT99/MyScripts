(function()local A=game:GetService("Players")local B=game:GetService("TeleportService")local C=game:GetService("HttpService")local D=game:GetService("ReplicatedStorage")Settings=Settings or {ServerHop=4,StudsUp=8,RetryDelay=2,EnableTeleport=true}local E=A.LocalPlayer local F=E.UserId local G=E.Character or E.CharacterAdded:Wait() local H=G:WaitForChild("HumanoidRootPart") local I=D:WaitForChild("data"):WaitForChild(tostring(F)) local J=I:WaitForChild("coins") local function K() if not Settings.EnableTeleport then return end local L=workspace:WaitForChild("tower"):WaitForChild("sections") local M=L:FindFirstChild("finish") if M and M:IsA("Model") then local N=M:FindFirstChild("start") if N and N:IsA("BasePart") then H.CFrame=N.CFrame+Vector3.new(0,Settings.StudsUp,0)end end end local function O() task.wait(Settings.ServerHop)local P={} local Q="" local R,S repeat R,S=pcall(function()return C:JSONDecode(game:HttpGet(string.format("https://games.roblox.com/v1/games/%d/servers/Public?sortOrder=Asc&limit=100%s",game.PlaceId,Q~="" and"&cursor="..Q or"")))end) if R and S and S.data then for _,T in ipairs(S.data)do if T.playing<T.maxPlayers and T.id~=game.JobId then table.insert(P,T.id)end end Q=S.nextPageCursor or"" else break end until Q=="" or#P>=5 if #P>0 then B:TeleportToPlaceInstance(game.PlaceId,P[math.random(1,#P)],E)else B:Teleport(game.PlaceId,E)end end local U=J.Value local V=false J:GetPropertyChangedSignal("Value"):Connect(function() if J.Value~=U then V=true;O() end end) task.spawn(function() while not V do K(); task.wait(Settings.RetryDelay) end end) end)()
